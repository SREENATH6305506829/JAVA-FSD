<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Phase2Project1.3-GenericServlets</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Generic</servlet-name>
		<servlet-class>com.generic.Generic</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Generic</servlet-name>
		<url-pattern>/Generic</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TempServlet</servlet-name>
		<servlet-class>com.generic.TempServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TempServlet</servlet-name>
		<url-pattern>/Temp</url-pattern>
	</servlet-mapping>
</web-app>